                // var arr = pidsdecodedData.split(',');
                const queryString = `?pids=${pids}&cid=${customer_id}&data=${selectedRoomInfo}`;

                const guestObj = [];
                var validationError = false;
                var firstErrorId = "";
                $(".firstname").each(function () {
                    var id = $(this).data('id');
                    var agegroup = $(this).data('agegroup');
                    var gtcnt = $(this).data('gtcnt');
                    var lastname = $('#guest_lastname' + gtcnt).val();

                    var fname = $(this).val();
                    var lname = $('#guest_lastname' + gtcnt).val();
                    var age = $('#guest_age' + gtcnt).val();

                    if (lname == '') {
                        validationError = true;
                        if (firstErrorId == '') {
                            firstErrorId = '#error_guest_lastname' + gtcnt;
                        }

                        $('#error_guest_lastname' + gtcnt).html('This field is required.');
                    } else {
                        if (!validationError) {
                            validationError = false;
                        }
                        $('#error_guest_lastname' + gtcnt).html('');
                    }

                    if (fname == '') {
                        validationError = true;
                        if (firstErrorId == '') {
                            firstErrorId = '#error_guest_firstname' + gtcnt;
                        }
                        $('#error_guest_firstname' + gtcnt).html('This field is required.');
                    } else {
                        if (!validationError) {
                            validationError = false;
                        }
                        $('#error_guest_firstname' + gtcnt).html('');
                    }

                    if (age == '') {
                        validationError = true;
                        if (firstErrorId == '') {
                            firstErrorId = '#error_guest_age' + gtcnt;
                        }
                        $('#error_guest_age' + gtcnt).html('This field is required.');
                    } else {
                        if (!validationError) {
                            validationError = false;
                        }
                        $('#error_guest_age' + gtcnt).html('');
                    }

                    const dynamicObject = {
                        fname: fname,
                        lname: lname,
                        age: age,
                        room_unique_id: id,
                        agegroup: agegroup
                    };
                    guestObj.push(dynamicObject);
                });


                // store selected data in localstorage
                localStorage.setItem('selectedPackageTicket', JSON.stringify(selectedValues));
                localStorage.setItem('guestInfo', JSON.stringify(guestObj));

                if (validationError) {


                    var target = $(firstErrorId);
                    if (target.length) {
                        $('html,body').animate({

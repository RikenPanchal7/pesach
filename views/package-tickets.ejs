    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Define a function to collect selected values
            function collectSelectedValues() {
                const selectedValues = [];

                // Iterate through all selects
                document.querySelectorAll('select').forEach(function (select) {
                    const room_id = select.getAttribute('data-room-id');
                    const room_unique_id = select.getAttribute('data-room-unique-id');
                    const room_price = select.getAttribute('data-room-price');
                    const early_room_price = select.getAttribute('data-early-room-price');
                    const package_id = select.getAttribute('data-package-id');
                    const name = select.getAttribute('data-name');
                    const room_name = select.getAttribute('data-room-name'); // Add this line
                    const price = select.getAttribute('data-price'); // Add this line
                    const selectedValue = select.value;

                    // if (selectedValue !== '0') {
                    selectedValues.push({
                        'name': name,
                        'room_id': room_id,
                        'room_unique_id': room_unique_id,
                        'room_price': room_price,
                        'early_room_price': early_room_price,
                        'package_id': package_id,
                        'selected_value': selectedValue,
                        'room_name': room_name,
                        'price': price,
                    });
                    // }
                });

                // Iterate through all checkboxes
                document.querySelectorAll('input[type="checkbox"]').forEach(function (checkbox) {
                    const room_id = checkbox.getAttribute('data-room-id');
                    const room_unique_id = checkbox.getAttribute('data-room-unique-id');
                    const room_price = checkbox.getAttribute('data-room-price');
                    const early_room_price = checkbox.getAttribute('data-early-room-price');
                    const package_id = checkbox.getAttribute('data-package-id');
                    const name = checkbox.getAttribute('data-name');
                    const room_name = checkbox.getAttribute('data-room-name');
                    const price = checkbox.getAttribute('data-price');
                    const isChecked = checkbox.checked;

                    selectedValues.push({
                        'name': name,
                        'room_price': room_price,
                        'early_room_price': early_room_price,
                        'room_id': room_id,
                        'room_unique_id': room_unique_id,
                        'package_id': package_id,
                        'is_checked': isChecked,
                        'room_name': room_name,
                        'price': price,

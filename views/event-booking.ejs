                                                <a href="#" class="quantity__minus"><span>-</span></a>
                                                <input name="quantity" type="text" class="quantity__input" value="0"
                                                    readonly>
                                                <a href="#" class="quantity__plus"><span>+</span></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- /cards -->
                            <!-- NEXT BUTTON-->
                            <div class="row d-flex mx-md-0 mx-3 align-items-center">
                                <button type="button"
                                    class="btn btn-dark fw-bold text-white back rounded-3 col-12 mb-3 mb-md-0 col-md-3 order-2 order-md-1">Previous</button>
                                <div
                                    class="col-12 col-md-6 d-flex justify-content-center mb-3 mb-md-0 order-1 order-md-2">
